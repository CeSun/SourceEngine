#include "common_vs_fxc.h"

struct VS_INPUT
{
	float4 vPos : SV_POSITION;
	float4 vColor : COLOR;
};

struct VS_OUTPUT
{
	float4 vPos : SV_POSITION;
	float4 vColor : COLOR;
};

VS_OUTPUT main( VS_INPUT i )
{
	VS_OUTPUT o = (VS_OUTPUT)0;
	o.vPos = mul( i.vPos, cModelViewProj );
	o.vColor = i.vColor;
	
	return o;
}