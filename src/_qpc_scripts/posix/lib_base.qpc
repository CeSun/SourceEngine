// ---------------------------------------------------------------
// Base Static Library - Posix
// ---------------------------------------------------------------
macro OUTLIBNAME	"$SCRIPT_NAME"	[!$OUTLIBNAME]
macro IS_LIB_PROJECT 	"1"

include "$SRCDIR/_qpc_scripts/posix_base.qpc"

configuration
{
	general
	{
		configuration_type "static_library"
	}

	linker
	{
		output_file "$OUTLIBDIR/$OUTLIBNAME"
	}
}
